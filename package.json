{
     "homepage": "https://chaseburr.github.io/Netflix-Clone",
     "name": "netflix-clone",
     "version": "0.1.0",
     "private": true,
     "dependencies": {
          "axios": "^0.21.1",
          "movie-trailer": "^2.0.7",
          "react": "^17.0.1",
          "react-dom": "^17.0.1",
          "react-router-dom": "^5.2.0",
          "react-youtube": "^7.13.1"
     },
     "scripts": {
          "prod": "npm run clean && webpack --mode=production",
          "dev": "npm run clean && webpack --config webpack.dev.js && webpack serve",
          "clean": "rimraf ./build/*",
          "predeploy": "npm run prod",
          "deploy": "gh-pages -d build"
     },
     "eslintConfig": {
          "extends": [
               "react-app",
               "react-app/jest"
          ]
     },
     "browserslist": {
          "production": [
               ">0.2%",
               "not dead",
               "not op_mini all"
          ],
          "development": [
               "last 1 chrome version",
               "last 1 firefox version",
               "last 1 safari version"
          ]
     },
     "devDependencies": {
          "@babel/core": "^7.12.13",
          "@babel/plugin-proposal-class-properties": "^7.12.13",
          "@babel/plugin-transform-runtime": "^7.12.15",
          "@babel/preset-env": "^7.12.13",
          "@babel/preset-react": "^7.12.13",
          "babel-loader": "^8.2.2",
          "css-loader": "^5.0.1",
          "dotenv-webpack": "^6.0.0",
          "gh-pages": "^3.1.0",
          "html-webpack-plugin": "^5.0.0",
          "rimraf": "^3.0.2",
          "sass": "^1.32.6",
          "sass-loader": "^11.0.0",
          "style-loader": "^2.0.0",
          "webpack": "^5.20.2",
          "webpack-cli": "^4.5.0",
          "webpack-dev-server": "^3.11.2"
     }
}
